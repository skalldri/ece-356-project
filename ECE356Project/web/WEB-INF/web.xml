<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>ece356.UserLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DatabaseTest</servlet-name>
        <servlet-class>ece356.DatabaseTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientMain</servlet-name>
        <servlet-class>patient.PatientMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoctorMain</servlet-name>
        <servlet-class>staff.DoctorMain</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VisitationRecords</servlet-name>
        <servlet-class>patient.VisitationRecords</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditPatientInformation</servlet-name>
        <servlet-class>patient.EditPatientInformation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatientSearch</servlet-name>
        <servlet-class>patient.PatientSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewDrug</servlet-name>
        <servlet-class>prescriptiondrugs.NewDrug</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DrugSearch</servlet-name>
        <servlet-class>prescriptiondrugs.DrugSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewPrescription</servlet-name>
        <servlet-class>prescriptiondrugs.NewPrescription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/UserLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DatabaseTest</servlet-name>
        <url-pattern>/DatabaseTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientMain</servlet-name>
        <url-pattern>/PatientMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoctorMain</servlet-name>
        <url-pattern>/DoctorMain</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VisitationRecords</servlet-name>
        <url-pattern>/VisitationRecords</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditPatientInformation</servlet-name>
        <url-pattern>/EditPatientInformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatientSearch</servlet-name>
        <url-pattern>/PatientSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewDrug</servlet-name>
        <url-pattern>/NewDrug</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DrugSearch</servlet-name>
        <url-pattern>/DrugSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewPrescription</servlet-name>
        <url-pattern>/NewPrescription</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
